<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DefineConstants>$(DefineConstants);AZUREAD</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="SecurityTokenResolver.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Abc.IdentityModel.Http\Abc.IdentityModel.Http.csproj" />
    <ProjectReference Include="..\Abc.IdentityModel.Protocols.Saml2\Abc.IdentityModel.Protocols.Saml.csproj" />
  </ItemGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="Abc.IdentityServer4.Saml2, " />
    <InternalsVisibleTo Include="Abc.IdentityServer4.Saml2.UnitTests, " />
    <InternalsVisibleTo Include="Abc.DuendeIdentityServer.Saml2, " />
    <InternalsVisibleTo Include="Abc.DuendeIdentityServer.Saml2.UnitTests, " />
  </ItemGroup>  
  
  <ItemGroup>
    <None Include="SecurityTokenResolver.cs" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="SR.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SR.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="SR.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>SR.Designer.cs</LastGenOutput>
      <CustomToolNamespace>Abc.IdentityModel.Http</CustomToolNamespace>
    </EmbeddedResource>
  </ItemGroup>

</Project>
